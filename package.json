{
  "name": "open-dart-dashboard",
  "version": "0.1.0",
  "private": true,
  "description": "Open DART API를 활용한 상장사 재무/배당 데이터 GitHub Pages 대시보드",
  "scripts": {
    "fetch:corp-code-list": "node scripts/fetch-corp-code-list.js",
    "fetch:corp-code-list:daily": "node scripts/ensure-corp-list-daily.js",
    "fetch:corp-index": "node scripts/fetch-corp-index.js",
    "fetch:overview": "node scripts/fetch-corp-overview.js",
    "fetch:financials": "node scripts/fetch-financials.js",
    "fetch:dividends": "node scripts/fetch-dividends.js",
    "fetch:all": "npm run fetch:corp-index && npm run fetch:overview && npm run fetch:financials && npm run fetch:dividends",
    "db:migrate": "node scripts/migrate-json-to-db.js",
    "db:sync-remote": "node scripts/db/sync-to-remote.js",
    "db:seed-targets": "node scripts/db/seed-targets.js"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "adm-zip": "^0.5.16",
    "cheerio": "^1.2.0",
    "dotenv": "^17.3.1"
  },
  "devDependencies": {}
}

